
<!DOCTYPE html>

<html>

<head>

    <meta charset="utf-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>E@T</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/css/LayoutCustom.css" rel="stylesheet" />

</head>

<body>

    <div class="navbar navbar-expand-sm navbar-inverse navbar-fixed-top" style="background-color:black;height:5%">
        <div class="container">
            <div class="navbar-header navbar-fixed-top">
                <a class="navbar-brand" style="color:white;position: absolute;left:25px;padding:0px"><img src="~/Content/Images/Master_Page/Capture.png" width="200" height="48" /></a>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                </button>
            </div>

            <div class="navbar-collapse collapse" style="color:white;position: absolute;right:25px;bottom:10px;">
                @Html.ActionLink("LOGOUT", "Logout", "User", null,new { @class = "btn btn-primary" })              
                @*<button type="button" class="btn btn-primary" onclick="">LOGOUT</button>*@
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")

    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)

    <div class="">

        @RenderBody()

        <hr />

    </div>
    <footer class="page-footer font-small" height=28>


        <div class="footer-copyright footer py-3">
            <img src="~/Content/Images/Master_Page/logo_white-blue.png" height=28 width=80 padding="6px" />© 2020 EPAMSystems.
        </div>

    </footer>

</body>

</html>